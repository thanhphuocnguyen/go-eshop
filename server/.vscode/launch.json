  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch file",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${file}",
      "envFile": "${workspaceFolder}/app.env",
      "cwd": "${workspaceFolder}"
    },
    {
      "name": "Launch Package",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/web",
      "envFile": "${workspaceFolder}/app.env",
      "args": [
        // "--profile",
        "api"
      ],
      "cwd": "${workspaceFolder}"
    },
    {
      "name": "Launch Migrate",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/migrate",
      "envFile": "${workspaceFolder}/app.env",
      "cwd": "${workspaceFolder}"
    },
    {
      "name": "Launch Seed",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/seed",
      "envFile": "${workspaceFolder}/app.env",
      "cwd": "${workspaceFolder}"
    }
  ]
}
