{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch file",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${file}",
      "envFile": "${workspaceFolder}/app.env",
      "cwd": "${workspaceFolder}"
    },
    {
      "name": "Launch API",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/web",
      "envFile": "${workspaceFolder}/app.env",
      "args": [
        // "--profile",
        "api"
      ],
      "cwd": "${workspaceFolder}"
    },
    {
      "name": "Launch Worker",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/web",
      "envFile": "${workspaceFolder}/app.env",
      "args": [
        // "--profile",
        "worker"
      ],
      "cwd": "${workspaceFolder}"
    },
    {
      "name": "Launch Migrate",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/migrate",
      "envFile": "${workspaceFolder}/app.env",
      "cwd": "${workspaceFolder}"
    },
    {
      "name": "Launch Seed",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/seed",
      "envFile": "${workspaceFolder}/app.env",
      "cwd": "${workspaceFolder}"
    },
    {
      "name": "Next.js: debug server-side",
      "type": "node-terminal",
      "request": "launch",
      "command": "npm run dev",
      "serverReadyAction": {
        "pattern": "- Local:.+(https?://.+:\\d+)",
        "uriFormat": "%s",
        "action": "debugWithEdge"
      }
    },
    {
      "name": "Next.js: debug client-side",
      "type": "msedge",
      "request": "launch",
      "url": "http://localhost:3000",
      "webRoot": "${workspaceFolder}",
      "runtimeExecutable": "/Applications/Microsoft Edge.app/Contents/MacOS/Microsoft Edge"
    },
    {
      "name": "Next.js: debug full stack",
      "type": "node-terminal",
      "request": "launch",
      "command": "npm run dev",
      "serverReadyAction": {
        "pattern": "- Local:.+(https?://.+:\\d+)",
        "uriFormat": "%s",
        "action": "debugWithEdge"
      },
      "console": "integratedTerminal",
      "skipFiles": ["<node_internals>/**"],
      "runtimeExecutable": "/Applications/Microsoft Edge.app/Contents/MacOS/Microsoft Edge"
    },
    {
      "name": "Next.js: debug production build",
      "type": "node-terminal",
      "request": "launch",
      "command": "npm run build"
    }
  ]
}
